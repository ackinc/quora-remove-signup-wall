{
  "name": "Quora - Remove Signup Wall",
  "description": "Remove Quora's annoying signup wall",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://*.quora.com/*"
      ],
      "js": [
        "index.js"
      ]
    }
  ],
  "incognito": "split",
  "permissions": [
    "*://*.quora.com/*"
  ]
}
